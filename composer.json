{
	"name": "sciactive/nymph-examples",
	"description": "Example apps built with Nymph.",
	"version": "1.0.0",
	"homepage": "http://nymph.io/",
	"authors": [
		{
			"name": "Hunter Perrin",
			"email": "hunter@sciactive.com",
			"homepage": "https://github.com/hperrin",
			"role": "Developer"
		}
	],
	"license": "LGPL",
	"require": {
		"sciactive/nymph-server": "dev-cleanup",
		"sciactive/nymph-pubsub": "dev-cleanup",
		"ext-pcntl": "*",
		"php": "~7.0"
	},
	"scripts": {
		"post-install-cmd": [
			"if [ -x \"$(npm bin -q)/bower\" ]; then $(npm bin -q)/bower install; else bower install; fi"
		],
		"post-update-cmd": [
			"if [ -x \"$(npm bin -q)/bower\" ]; then $(npm bin -q)/bower install; else bower install; fi"
		]
	},
	"minimum-stability": "dev"
}
