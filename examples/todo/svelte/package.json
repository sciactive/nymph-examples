{
  "name": "nymph-examples-todo-svelte",
  "version": "1.0.0",
  "description": "An example todo app using Nymph and Svelte.",
  "main": "index.php",
  "scripts": {
    "build": "npm run build-TodoApp && npm run build-TodoEl",
    "build-TodoApp": "svelte compile --format iife src/TodoApp.html > build/TodoApp.js",
    "build-TodoEl": "svelte compile --format iife src/TodoEl.html > build/TodoEl.js",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "svelte": "^1.22.4",
    "svelte-cli": "^1.3.7"
  }
}
